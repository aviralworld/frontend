{
  "name": "frontend",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "format:prettier": "npx prettier --write --plugin-search-dir=. src/**/*.ts src/**/*.js src/**/*.svelte",
    "format:eslint": "npm run lint:fix",
    "format": "npm run format:prettier && npm run format:eslint",
    "lint": "eslint src --ext=js,jsx,ts",
    "lint:fix": "eslint src --ext=js,jsx,ts --fix",
    "start": "node __sapper__/build",
    "test": "npm run build && run-p --race start test:integration",
    "test:integration": "mocha -r ts-node/register test/**/*.spec.ts",
    "validate": "svelte-check --ignore src/node_modules/@sapper"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "compression": "^1.7.1",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.2",
    "roarr": "^2.15.4",
    "serialize-error": "^7.0.1",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/chai": "^4.2.14",
    "@types/compression": "^1.7.0",
    "@types/dom-mediacapture-record": "^1.0.7",
    "@types/dotenv-safe": "^8.1.1",
    "@types/express": "^4.17.8",
    "@types/express-http-proxy": "^1.6.1",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.11.1",
    "@types/pg": "^7.14.7",
    "@types/puppeteer": "^5.4.2",
    "@types/roarr": "^2.14.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "chai": "^4.2.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "mocha": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "pg": "^8.5.1",
    "prettier": "^2.1.2",
    "prettier-plugin-svelte": "^1.4.0",
    "puppeteer": "^5.5.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "svelte": "^3.17.3",
    "svelte-check": "^1.0.59",
    "svelte-preprocess": "^4.3.0",
    "ts-node": "^9.1.1",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  }
}
